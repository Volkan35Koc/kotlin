<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ortalamahp"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="12dp">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="1106dp"
        android:layout_height="1298dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.515"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/arkaplan"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="-522dp" />

    <ImageView
        android:id="@+id/obpbos"
        android:layout_width="115dp"
        android:layout_height="66dp"

        android:layout_marginTop="72dp"
        android:layout_marginEnd="68dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.892"
        app:layout_constraintStart_toEndOf="@+id/sınıf9"
        app:layout_constraintTop_toTopOf="@+id/textView7"
        app:srcCompat="@android:drawable/editbox_background_normal"
        tools:ignore="MissingConstraints,NotSibling" />

    <ImageView
        android:id="@+id/obpbos1"
        android:layout_width="115dp"
        android:layout_height="66dp"

        android:layout_marginEnd="68dp"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/buttono"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/sınıf12"
        app:layout_constraintTop_toBottomOf="@+id/obpbos2"
        app:layout_constraintVertical_bias="0.9"
        app:srcCompat="@android:drawable/editbox_background_normal"
        tools:ignore="MissingConstraints,NotSibling" />

    <ImageView
        android:id="@+id/obpbos2"
        android:layout_width="115dp"
        android:layout_height="66dp"

        android:layout_marginTop="12dp"
        android:layout_marginEnd="68dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/sınıf11"
        app:layout_constraintTop_toBottomOf="@+id/obpbos3"
        app:srcCompat="@android:drawable/editbox_background_normal"
        tools:ignore="MissingConstraints,NotSibling" />

    <ImageView
        android:id="@+id/obpbos3"
        android:layout_width="115dp"
        android:layout_height="66dp"

        android:layout_marginTop="20dp"
        android:layout_marginEnd="68dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/sınıf10"
        app:layout_constraintTop_toBottomOf="@+id/obpbos"
        app:srcCompat="@android:drawable/editbox_background_normal"
        tools:ignore="MissingConstraints,NotSibling" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar4"
        android:layout_width="105dp"
        android:layout_height="49dp"
        android:layout_marginBottom="140dp"
        android:background="@color/white"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.404"
        app:layout_constraintStart_toEndOf="@+id/buttono" />


    <Button
        android:id="@+id/buttono"
        android:layout_width="102dp"
        android:layout_height="56dp"
        android:layout_marginStart="68dp"
        android:layout_marginBottom="136dp"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/sınıf9"
        android:layout_width="148dp"
        android:layout_height="47dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="200dp"
        android:fontFamily="@font/ubuntu"
        android:text="9.sınıf :"
        android:textAlignment="viewEnd"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        android:textStyle="italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/sınıf10"
        android:layout_width="148dp"
        android:layout_height="46dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="56dp"
        android:fontFamily="@font/ubuntu"
        android:text="10.sınıf :"
        android:textAlignment="viewEnd"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        android:textStyle="italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sınıf9" />

    <TextView
        android:id="@+id/sınıf11"
        android:layout_width="149dp"
        android:layout_height="48dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/ubuntu"
        android:text="11.sınıf :"
        android:textAlignment="viewEnd"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        android:textStyle="italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sınıf10" />

    <TextView
        android:id="@+id/sınıf12"
        android:layout_width="147dp"
        android:layout_height="48dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="36dp"
        android:fontFamily="@font/ubuntu"
        android:text="12.sınıf :"
        android:textAlignment="viewEnd"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        android:textStyle="italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sınıf11" />


    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="576dp"
        android:fontFamily="@font/ubuntu_bold"
        android:text="OBP Hesaplama"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/obpo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar4"
        app:layout_constraintEnd_toEndOf="@+id/toolbar4"
        app:layout_constraintHorizontal_bias="0.441"
        app:layout_constraintStart_toStartOf="@+id/toolbar4"
        app:layout_constraintTop_toTopOf="@+id/toolbar4"
        app:layout_constraintVertical_bias="0.25" />

    <EditText
        android:id="@+id/editTextNumberDecimal"
        android:layout_width="89dp"
        android:layout_height="44dp"
        android:layout_marginTop="88dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.103"
        app:layout_constraintStart_toStartOf="@+id/obpbos"
        app:layout_constraintTop_toTopOf="@+id/textView7"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editTextNumberDecimal2"
        android:layout_width="99dp"
        android:layout_height="44dp"
        android:layout_marginTop="40dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintEnd_toEndOf="@+id/obpbos3"
        app:layout_constraintHorizontal_bias="0.437"
        app:layout_constraintStart_toStartOf="@+id/obpbos3"
        app:layout_constraintTop_toBottomOf="@+id/editTextNumberDecimal"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editTextNumberDecimal3"
        android:layout_width="97dp"
        android:layout_height="43dp"
        android:layout_marginTop="40dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintEnd_toEndOf="@+id/obpbos2"
        app:layout_constraintStart_toStartOf="@+id/obpbos2"
        app:layout_constraintTop_toBottomOf="@+id/editTextNumberDecimal2"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editTextNumberDecimal4"
        android:layout_width="99dp"
        android:layout_height="45dp"
        android:layout_marginTop="24dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.083"
        app:layout_constraintStart_toStartOf="@+id/obpbos1"
        app:layout_constraintTop_toBottomOf="@+id/editTextNumberDecimal3"
        app:layout_constraintVertical_bias="0.076"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>